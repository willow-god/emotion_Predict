   "source": [
    "# 读取文件data/train2.csv,将label列数据中，高于4的设置为1，低于4的设置为0\n",
    "\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "# 读取文件\n",
    "train = pd.read_csv('data/train2.csv')\n",
    "# 将label列数据中，高于4的设置为1，低于4的设置为0\n",
    "train['label'] = np.where(train['label'] >= 4, 1, 0)\n",
    "# 保存文件\n",
    "train.to_csv('data/train.csv', index=False)"
   ]
